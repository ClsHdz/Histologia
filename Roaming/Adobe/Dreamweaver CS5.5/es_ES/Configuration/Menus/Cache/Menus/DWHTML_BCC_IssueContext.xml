<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/">
<menuitem key="Cmd+D" file="Menus/MM/Open_Document.htm" id="DWHTMLContext_BCCIssue_OpenDocument" dynamic="true" />
<separator />
<menuitem name="_Comprobar compatibilidad con navegadores" command="dw.getDocumentDOM().runBCC('auto-explicit')" enabled="dw.getDocumentDOM() != null && dw.getDocumentDOM().documentType != 'XML'" domRequired="TRUE" id="DWHTMLContext_BCCIssue_Refresh" />
<menuitem name="Ignorar problema" command="dw.getDocumentDOM().source.ignoreIssue(dw.getDocumentDOM().source.getSelection()[0]);dw.getDocumentDOM().runBCC('auto-explicit')" enabled="dw.getDocumentDOM() != null && dw.getDocumentDOM().source.canIgnoreIssue(dw.getDocumentDOM().source.getSelection()[0])" id="DWHTMLContext_BCCIssue_IgnoreError" />
<menuitem name="_Siguiente problema" arguments="'forward'" file="Menus/MM/Goto_ValError.htm" domRequired="FALSE" id="DWHTMLContext_BCCIssue_NextIssue" />
<menuitem name="_Problema anterior" arguments="'back'" file="Menus/MM/Goto_ValError.htm" domRequired="FALSE" id="DWHTMLContext_BCCIssue_PrevIssue" />
<menuitem name="_Mostrar todos los problemas" arguments="'showresults'" file="Menus/MM/Goto_ValError.htm" domRequired="FALSE" id="DWHTMLContext_BCCIssue_Results" />
<separator />
<menuitem name="_Editar lista de problemas ignorados" command="dw.editBTCExceptions()" enabled="true" domRequired="FALSE" id="DWHTMLContext_BCCIssue_EditIgnoredIssues" />
<menuitem name="Configuración..." command="if (dw.showTargetBrowsersDialog() && dw.getDocumentDOM() && dw.getDocumentDOM().documentType != 'XML') dw.getDocumentDOM().runBCC('auto-explicit',true)" enabled="true" id="DWHTMLContext_BCCIssue_Options" />
