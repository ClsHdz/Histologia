<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/">
<menuitem name="_Traer al frente" command="dw.getDocumentDOM().arrange('front')" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canArrange()" id="DWMenu_Modify_Arrange_BringFront" />
<menuitem name="En_viar al fondo" command="dw.getDocumentDOM().arrange('back')" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canArrange()" id="DWMenu_Modify_Arrange_SendBack" />
<menuitem name="A_linear a la izquierda" key="Cmd+Shift+1" command="dw.getDocumentDOM().align('left')" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canAlign()" id="DWMenu_Modify_Align_AlignLeft" />
<menuitem name="Alinea_r a la derecha" key="Cmd+Shift+3" command="dw.getDocumentDOM().align('right')" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canAlign()" platform="win" id="DWMenu_Modify_Align_AlignRight" />
<menuitem name="Alinear con el borde su_perior" key="Cmd+Shift+4" command="dw.getDocumentDOM().align('top')" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canAlign()" platform="win" id="DWMenu_Modify_Align_AlignTop" />
<menuitem name="Alinear con el _borde inferior" key="Cmd+Shift+6" command="dw.getDocumentDOM().align('bottom')" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canAlign()" id="DWMenu_Modify_Align_AlignBottom" />
<separator />
<menuitem name="Asignar mismo anc_ho" key="Cmd+Shift+7" command="dw.getDocumentDOM().makeSizesEqual(true,false)" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canAlign()" id="DWMenu_Modify_Align_MakeSameWidth" />
<menuitem name="Asignar mismo al_to" key="Cmd+Shift+9" command="dw.getDocumentDOM().makeSizesEqual(false,true)" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canAlign()" id="DWMenu_Modify_Align_MakeSameHeight" />
<separator />
<menuitem name="_Evitar solapamientos de elementos PA" command="dw.getDocumentDOM().setPreventLayerOverlaps(!dw.getDocumentDOM().getPreventLayerOverlaps())" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM() != null && dw.getDocumentDOM().getFocus() == 'body'" checked="dw.getDocumentDOM() != null && dw.getDocumentDOM().getPreventLayerOverlaps()" id="DWMenu_Modify_PreventOverlaps" />
